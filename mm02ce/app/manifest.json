{
  "name": "maomao - share & discover",
  "short_name": "maomao",
  "version": "0.6.15",
  "description": "maomao is a peer-to-peer real time content sharing network, powered by a deep learning engine.",
  "background": {
    "page": "bg.html"
  },
  "icons": {
    "128": "img/logo/maomao_blue.png"
  },
  "web_accessible_resources": [
    "img/*.*",
    "build/*.*",
    "css/*.*"
  ],
  "content_security_policy": "script-src 'self' 'unsafe-eval' https://cdn.auth0.com blob: filesystem: chrome-extension-resource: https://www.gstatic.com/ https://*.firebaseio.com https://*.pusher.com https://*.google.com https://*.googleapis.com https://connect.facebook.net; object-src 'self'",
  "permissions": [
    "idle",
    "webRequest",
    "contextMenus",
    "activeTab",
    "tabs",
    "bookmarks",
    "webNavigation",
    "storage",
    "unlimitedStorage",
    "identity",
    "notifications",
    "https://ssl.gstatic.com/",
    "*://*.googleapis.com/",
    "*://*.google.com/*",
    "https://dunghd.auth0.com/*",
    "http://*/*",
    "https://*/*"
  ],
  "content_scripts": [
    {
      "matches": [
        "*://*/*"
      ],
      "css": [
        "./build/styles.css"
      ],
      "js": [
        "./build/content.js"
      ],
      "run_at": "document_end"
    }
  ],
  "externally_connectable": {
    "matches": [
      "*://maomaoweb.azurewebsites.net/*",
      "*://maomaoweb-dev.azurewebsites.net/*",
      "*://maomao.rocks/*",
      "*://www.maomao.rocks/*",
      "http://localhost:3000/*"
    ]
  },
  "browser_action": {
    "default_title": "maomao - share your stream with friends",
    "default_icon": "img/icon.png",
    "default_popup": "build/popup.html"
  },
  "oauth2": {
    "client_id": "323116239222-rgs4qqmu234iafmtg1s5jmbhpkppbv8a.apps.googleusercontent.com",
    "scopes": [
      "https://www.googleapis.com/auth/plus.me",
      "https://www.googleapis.com/auth/userinfo.email",
      "https://www.googleapis.com/auth/contacts.readonly"
    ]
  },
  "homepage_url": "http://www.maomao.rocks",
  "key": "",
  "manifest_version": 2
}
